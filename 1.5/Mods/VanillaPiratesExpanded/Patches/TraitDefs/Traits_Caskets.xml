<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs</xpath>
			<value>
				<TraitDef>
					<defName>VFEP_WarcasketTrait_Mech</defName>
					<commonality>0</commonality>
					<disabledWorkTags>
						<li>ManualDumb</li>
						<li>Animals</li>
						<li>PlantWork</li>
						<li>Artistic</li>
						<li>Mining</li>
					</disabledWorkTags>
					<degreeDatas>
						<li>
							<label>mech warcasket</label>
							<description>{PAWN_nameDef} is entombed in a mech master warcasket. No amount of pain will have an effect on them, and they can never go into a pain shock or drop unconscious due to pain. {PAWN_nameDef} will be unable to do most tasks, except those necessary to maintain and build mechanoids.</description>
						</li>
					</degreeDatas>
					<modExtensions>
						<li Class="VFECore.TraitExtension">
							<painFactor>0</painFactor>
						</li>
					</modExtensions>
				</TraitDef>
			</value>
		</match>
	</Operation>
</Patch>
